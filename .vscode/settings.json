{
    "ansible.python.activationScript": ".venv/bin/activate",
    "ansible.validation.lint.arguments": "-c .ansible-lint.yml",
    "files.associations": {
        "*.yml": "ansible",
        "*.j2": "ansible-jinja",
        "config.js":"json"
    },
    "files.autoSave": "onFocusChange",
    "gitlens.currentLine.enabled": false,
    "explorer.excludeGitIgnore": true,
    "python.terminal.activateEnvironment": false,
    "yaml.schemas": {
        "https://github.com/datreeio/CRDs-catalog/raw/main/traefik.io/ingressroute_v1alpha1.json": [
            "ingress-route.yaml"
        ],
        "https://github.com/datreeio/CRDs-catalog/raw/main/traefik.io/ingressroutetcp_v1alpha1.json": [
            "ingress-route-tcp.yaml"
        ],
        "https://github.com/datreeio/CRDs-catalog/raw/main/cert-manager.io/certificate_v1.json": [
            "certificate.yaml"
        ],
        "https://github.com/datreeio/CRDs-catalog/raw/main/argoproj.io/application_v1alpha1.json": [
            "cluster/bootstrap/*.yaml"
        ],
        "kubernetes": []
    },
    "ansible.python.interpreterPath": ".venv/bin/python",
    "ansible.ansible.path": ".venv/bin/ansible",
    "ansible.ansibleNavigator.path": ".venv/bin/ansible-navigator"
}
